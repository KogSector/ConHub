{
  "id": "google-drive",
  "name": "Google Drive Connector",
  "version": "1.0.0",
  "description": "Connector for accessing Google Drive files and documents",
  "capabilities": [
    "documents",
    "files",
    "search",
    "metadata"
  ],
  "metadata": {
    "category": "cloud-storage",
    "provider": "google",
    "supportedFormats": ["docs", "sheets", "slides", "pdf", "txt"],
    "rateLimits": {
      "requestsPerMinute": 100,
      "requestsPerDay": 10000
    }
  },
  "authentication": {
    "type": "oauth2",
    "scopes": [
      "https://www.googleapis.com/auth/drive.readonly",
      "https://www.googleapis.com/auth/documents.readonly"
    ]
  },
  "settings": {
    "maxFileSize": "50MB",
    "supportedMimeTypes": [
      "application/vnd.google-apps.document",
      "application/vnd.google-apps.spreadsheet",
      "application/vnd.google-apps.presentation",
      "application/pdf",
      "text/plain"
    ]
  }
}