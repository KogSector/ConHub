{
  "id": "amazon-q",
  "name": "Amazon Q",
  "version": "1.0.0",
  "description": "amazon-q connector",
  "capabilities": [
    "chat",
    "completion"
  ],
  "metadata": {
    "category": "ai-agent",
    "provider": "amazon-q",
    "supportedFormats": [
      "text",
      "documents"
    ],
    "rateLimits": {
      "requestsPerMinute": 60,
      "requestsPerHour": 1000
    }
  },
  "authentication": {
    "type": "none"
  },
  "settings": {
    "maxFileSize": "100MB"
  }
}