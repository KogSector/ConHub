# Indexers Service Environment Variables

# Embedding service
EMBEDDING_SERVICE_URL=http://embedding:8082

# Qdrant vector database
QDRANT_URL=http://qdrant:6333
QDRANT_TIMEOUT_SECS=5

# Fusion search parameters
FUSION_KEYWORD_LIMIT=100
FUSION_VECTOR_LIMIT=100
FUSION_RERANK_LIMIT=100
FUSION_FINAL_LIMIT=20
FUSION_RRF_K=60

# Vector indexing
ENABLE_VECTOR_INDEXING=true
