{
  "db_name": "PostgreSQL",
  "query": "\n                SELECT \n                    ca.id,\n                    ca.user_id,\n                    ca.connector_type,\n                    ca.account_name,\n                    ca.account_identifier,\n                    ca.status,\n                    ca.last_sync_at,\n                    ca.created_at,\n                    ca.updated_at,\n                    COALESCE(COUNT(sd.id), 0)::int as document_count\n                FROM connected_accounts ca\n                LEFT JOIN source_documents sd ON sd.source_id = ca.id\n                WHERE ca.id = $1 AND ca.user_id = $2\n                GROUP BY ca.id\n                ",
