{
  "profiles": [
    {
      "id": "github_code_rust",
      "match": {
        "connector_type": "github",
        "block_type": "code",
        "language": "rust"
      },
      "models": ["code_primary"],
      "weights": [1.0],
      "fusion_strategy": "weighted_average",
      "chunker": "code_functions",
      "description": "Optimized for Rust code in GitHub repositories"
    },
    {
      "id": "github_code_python",
      "match": {
        "connector_type": "github",
        "block_type": "code",
        "language": "python"
      },
      "models": ["code_primary"],
      "weights": [1.0],
      "fusion_strategy": "weighted_average",
      "chunker": "code_functions",
      "description": "Optimized for Python code in GitHub repositories"
    },
    {
      "id": "github_code_typescript",
      "match": {
        "connector_type": "github",
