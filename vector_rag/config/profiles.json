  "fallback_profile": {
    "id": "default_fallback",
    "models": ["general_text"],
    "weights": [1.0],
    "fusion_strategy": "weighted_average",
    "chunker": "generic_paragraphs",
    "description": "Default fallback for unmatched content"
  },
  "content_type_detection": {
    "code_extensions": [".rs", ".py", ".js", ".ts", ".tsx", ".jsx", ".go", ".java", ".c", ".cpp", ".h", ".hpp", ".cs", ".rb", ".php", ".swift", ".kt", ".scala", ".sh", ".sql", ".r"],
    "text_extensions": [".md", ".txt", ".rst", ".adoc", ".tex", ".rtf"],
    "language_mapping": {
      ".rs": "rust",
      ".py": "python",
      ".js": "javascript",
      ".ts": "typescript",
      ".tsx": "typescript",
      ".jsx": "javascript",
      ".go": "go",
      ".java": "java",
      ".c": "c",
