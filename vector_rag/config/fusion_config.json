{
  "models": [
    {
      "name": "general_text",
      "client": "huggingface",
      "model": "sentence-transformers/all-MiniLM-L6-v2",
      "dimension": 384,
      "strengths": ["code", "github", "gitlab", "bitbucket", "local_file", "docs", "slack", "urls", "graph_chunks"]
    }
  ],
  "routing": [
    {
      "source": "github",
      "models": ["general_text"],
      "weights": [1.0],
      "fusion_strategy": "weighted_average"
    },
    {
      "source": "graph_chunks",
      "models": ["general_text"],
      "weights": [1.0],
      "fusion_strategy": "weighted_average"
    },
    {
      "source": "code",
      "models": ["general_text"],
      "weights": [1.0],
      "fusion_strategy": "weighted_average"
    },
    {
      "source": "docs",
      "models": ["general_text"],
      "weights": [1.0],
      "fusion_strategy": "weighted_average"
    }
  ],
  "fallback_model": "general_text",
  "cache_embeddings": true,
  "normalize_embeddings": true,
  "batch_size": 32,
  "max_retries": 3,
  "timeout_seconds": 30
}
