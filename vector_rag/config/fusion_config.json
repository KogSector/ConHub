{
  "models": [
    {
      "name": "code_primary",
      "client": "qwen",
      "model": "text-embedding-v3",
      "dimension": 1536,
      "strengths": ["code", "github", "gitlab", "bitbucket", "local_file", "docs", "slack", "urls"]
    }
  ],
  "routing": [
    {
      "source": "github",
      "models": ["code_primary"],
      "weights": [1.0],
      "fusion_strategy": "weighted_average"
