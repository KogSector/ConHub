{
  "id": "github-copilot",
  "name": "Github Copilot",
  "version": "1.0.0",
  "description": "github-copilot connector",
  "capabilities": [
    "chat",
    "completion",
    "code-generation",
    "code-analysis"
  ],
  "metadata": {
    "category": "ai-agent",
    "provider": "github-copilot",
    "supportedFormats": [
      "text",
      "documents"
    ],
    "rateLimits": {
      "requestsPerMinute": 60,
      "requestsPerHour": 1000
    }
  },
  "authentication": {
    "type": "none"
  },
  "settings": {
    "maxFileSize": "100MB"
  }
}